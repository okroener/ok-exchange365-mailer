    "type": "typo3-cms-extension",
    "license": "GPL-2.0-or-later",
    "version": "3.0.0",
    "authors": [
        {
            "name": "Oliver Kroener",
            "email": "ok@oliver-kroener.de"
        }
    ],
    "require": {
        "typo3/cms-core": "^11.5 || ^12.4 || ^13 || dev-main",
        "oliverkroener/ok-typo3-helper": "^2",
        "microsoft/microsoft-graph": "^2"
    },
    "autoload": {
        "psr-4": {
